{
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "denoland.vscode-deno",
    },
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.aleph": true
    },
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": true,
    "deno.import_map": "./import_map.json",
    "cSpell.words": [
        "Biblio",
        "bibliophilekass"
    ]
}